{
  "name": "salt-react",
  "version": "1.0.0",
  "description": "saltframework with react",
  "author": "Syaku (최석균) <http://syaku.tistory.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "config": {
    "port": 8088,
    "publicPath": "/",
    "apiPath": "/api",
    "proxyHost": "http://localhost:8080",
    "output": "dist",
    "src": "src",
    "vendors": [
      "babel-polyfill",
      "react",
      "react-dom",
      "react-addons-update",
      "react-router",
      "react-router-dom",
      "redux",
      "react-redux",
      "qs",
      "axios",
      "store",
      "nprogress",
      "immutable"
    ]
  },
  "scripts": {
    "build:dev": "webpack --colors --progress -d",
    "build:prod": "webpack --config webpack.deploy.config.js --colors --progress -p",
    "serv:dev": "webpack-dev-server --colors --progress -d",
    "serv:dev2": "webpack-dev-server --config webpack.deploy.config.js --colors --progress -d",
    "serv:prod": "webpack-dev-server --colors --progress -p --compress",
    "state:dev": "webpack --config webpack.state.config.js --colors --progress -d --profile --json > stats-dev.json",
    "state:prod": "webpack --config webpack.state.config.js --colors --progress -p --profile --json > stats.json"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "bundle-loader": "^0.5.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.2",
    "style-loader": "^0.18.2",
    "stylelint": "7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "ckeditor": "https://github.com/ckeditor/ckeditor-releases.git#full/4.7.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "font-awesome": "^4.7.0",
    "hover.css": "^2.2.0",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "is-class": "^0.0.4",
    "lodash": "^4.17.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.10",
    "qs": "^6.3.1",
    "react": "15.6.1",
    "react-addons-pure-render-mixin": "^15.6.0",
    "react-addons-update": "^15.4.1",
    "react-dom": "15.6.1",
    "react-grid-layout": "^0.14.7",
    "react-modal": "^2.2.2",
    "react-redux": "^5.0.3",
    "react-resizable": "^1.7.1",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "shortid": "^2.2.8",
    "store": "^2.0.4",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "^2.3.0"
  }
}
